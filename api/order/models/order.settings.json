{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "paymentId": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "failed",
        "success",
        "visited"
      ]
    },
    "consumer": {
      "type": "component",
      "repeatable": false,
      "component": "event.recipient"
    },
    "event": {
      "model": "event",
      "via": "orders"
    },
    "ticketReference": {
      "type": "component",
      "repeatable": true,
      "component": "event.ticket-reference"
    },
    "ip": {
      "type": "string"
    },
    "reference": {
      "type": "string"
    },
    "amount": {
      "type": "decimal"
    },
    "timestamp": {
      "type": "string"
    },
    "paymentType": {
      "type": "string"
    },
    "paymentMethod": {
      "type": "string"
    }
  }
}
